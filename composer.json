{
  "name": "ahmed-hussain70/crud-generator",
  "description": "A simple CRUD generator for Laravel",
  "type": "library",
  "require": {
      "ahmed-hussain70/crud-generator": "dev-main"
  },
  "autoload": {
      "psr-4": {
          "ahmed-hussain\\crud-generator\\": "src/"
      }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Ahmedhussain70\\CrudGenerator\\CrudGeneratorServiceProvider"
          ]
      }
  },
  "scripts": {
      "post-install-cmd": [
          "php artisan vendor:publish --provider=\"Ahmedhussain70\\CrudGenerator\\CrudGeneratorServiceProvider\""
      ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
