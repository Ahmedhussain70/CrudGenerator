{
  "name": "Ahmedhussain70/CrudGenerator",
  "description": "A simple CRUD generator for Laravel",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "Ahmedhussain70/CrudGenerator": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "CrudGenerator\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "./CrudGenerator"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Ahmedhussain70\\CrudGenerator\\CrudGeneratorServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan vendor:publish --provider=\"Ahmedhussain70\\CrudGenerator\\CrudGeneratorServiceProvider\""
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
