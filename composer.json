{
  "name": "ahmed-hussain70/crud-generator",
  "description": "A simple CRUD generator for Laravel",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ahmed-hussain70/crud-generator": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "CrudGenerator\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "./crud-generator"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "ahmed-hussain70\\crud-generator\\CrudGeneratorServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan vendor:publish --provider=\"ahmed-hussain70\\crud-generator\\CrudGeneratorServiceProvider\""
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
